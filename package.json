{
  "name": "spotify-clone",
  "homepage": "http://iakindev.github.io/spotify-clone",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "format": "yarn run format:ts && yarn run format:css",
    "format:check": "prettier -c 'src/**/*.{ts,tsx}'",
    "lint": "yarn run lint:ts && yarn run lint:css",
    "lint:ts": "tsc && yarn lint:eslint",
    "lint:eslint": "eslint 'src/**/*.{ts,tsx}'",
    "format:ts": "prettier --write 'src/**/*.{ts,tsx}' && yarn lint:eslint --fix",
    "lint:css": "stylelint --syntax scss \"src/**/*.scss\"",
    "format:css": "stylelint --fix --syntax scss \"src/**/*.scss\"",
    "coverage": "yarn run test --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "gen-icon-components": "svgr --typescript src/assets/icons --icon --replace-attr-values \"#000=currentColor\" -d src/components/icons",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@babel/helper-call-delegate": "^7.12.1",
    "@craco/craco": "^6.0.0",
    "@material-ui/core": "^4.11.2",
    "@material-ui/styles": "^4.11.2",
    "@modulz/radix-icons": "^3.3.0",
    "axios": "^0.21.0",
    "country-list": "^2.2.0",
    "craco-plugin-scoped-css": "^1.1.0",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "query-string": "^6.13.7",
    "react": "^17.0.1",
    "react-date-picker": "^8.0.5",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.13.0",
    "react-howler": "^5.0.0",
    "react-image-fade-in": "^1.1.3",
    "react-input-slider": "^6.0.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-spinners-kit": "^1.9.1",
    "typescript": "~4.1.3"
  },
  "devDependencies": {
    "@atolye15/eslint-config": "^1.2.0",
    "@atolye15/stylelint-config": "^1.3.1",
    "@babel/core": "^7.12.10",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/addons": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@svgr/cli": "^5.5.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/country-list": "^2.1.0",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/howler": "^2.2.1",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-howler": "^3.7.2",
    "@types/react-router-dom": "^5.1.6",
    "babel-loader": "^8.2.2",
    "customize-cra": "^1.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.15.0",
    "gh-pages": "^3.1.0",
    "node-sass": "4.14.1",
    "prettier": "^2.2.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-test-renderer": "^17.0.1",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-prettier": "^1.1.2"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx",
      "!src/setupTests.ts",
      "!src/components/**/index.{ts,tsx}",
      "!src/components/**/*.stories.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
