{
  "name": "spotify-clone",
  "homepage": "http://iakindev.github.io/spotify-clone",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@babel/helper-call-delegate": "^7.10.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/howler": "^2.2.1",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-howler": "^3.7.2",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-howler": "^3.7.4",
    "react-input-slider": "^6.0.0",
    "react-scripts": "3.4.1",
    "react-spinners-kit": "^1.9.1",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "format": "yarn run format:ts && yarn run format:css",
    "format:check": "prettier -c 'src/**/*.{ts,tsx}'",
    "lint": "yarn run lint:ts && yarn run lint:css",
    "lint:ts": "tsc && yarn lint:eslint",
    "lint:eslint": "eslint 'src/**/*.{ts,tsx}'",
    "format:ts": "prettier --write 'src/**/*.{ts,tsx}' && yarn lint:eslint --fix",
    "lint:css": "stylelint --syntax scss \"src/**/*.scss\"",
    "format:css": "stylelint --fix --syntax scss \"src/**/*.scss\"",
    "coverage": "yarn run test --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "gen-icon-components": "svgr --typescript src/img --icon --replace-attr-values \"#000=currentColor\" -d src/components",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx",
      "!src/setupTests.ts",
      "!src/components/**/index.{ts,tsx}",
      "!src/components/**/*.stories.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@atolye15/eslint-config": "^1.1.1",
    "@atolye15/stylelint-config": "^1.3.0",
    "@babel/core": "^7.10.2",
    "@hot-loader/react-dom": "^16.13.0",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@svgr/cli": "^5.4.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "babel-loader": "^8.1.0",
    "customize-cra": "^1.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.2.0",
    "gh-pages": "^3.1.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "react-app-rewired": "^2.1.6",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-test-renderer": "^16.13.1",
    "stylelint": "^13.6.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-prettier": "^1.1.2"
  }
}
